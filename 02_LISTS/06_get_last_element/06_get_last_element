def get_last_element():
    my_list: list[str] = []

    try:
        n = int(input("ğŸ”¢ How many elements do you want to enter? "))

        if n <= 0:
            print("âš ï¸ Please enter a number greater than 0.")
            return

        for i in range(n):
            element = input(f"Enter element {i + 1} (or press Enter to stop early): ")
            if element == "":
                if my_list:
                    print(f"âœ… Your last entered element is: {my_list[-1]}")
                else:
                    print("âš ï¸ No elements were added.")
                return
            my_list.append(element)

        print(f"âœ… Your last entered element is: {my_list[-1]}")

    except ValueError:
        print("âŒ Invalid input. Please enter a valid number.")

def main():
    get_last_element()

if __name__ == '__main__':
    main()
